@model List<Category>

<h1>Category List </h1>
<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>
            Category Name
        </th>
        <th>
            Display Order 
        </th>
    </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model.OrderBy(u=>u.DesplayOrder))
        {
            <tr>
                <td>@obj.Name</td>
            <td>
                    @obj.DesplayOrder
            </td>
        </tr>
        }
    </tbody>
</table>